                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                },
                "_openLinkToTasmota": {
                    "type": "sendTo",
                    "label": "openLinkToTasmota",
                    "jsonData": "{\"ip\": \"${data._selectIpForTools}\"}",
                    "openUrl": true,
                    "command": "openLinkToTasmota",
                    "disabled": "!data._selectIpForTools",
                    "variant": "outlined",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                },
                "_berryInstallSendTo": {
                    "type": "sendTo",
                    "label": "berryInstallSendTo",
                    "jsonData": "{\"tasmotaIP\": \"${data._selectIpForTools}\"}",
                    "showProcess": true,
                    "command": "berryInstallSendTo",
                    "disabled": "!data._selectIpForTools || data._selectIpForTools.search(/^[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}$/g) === -1",
                    "variant": "contained",
                    "newLine": true,
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                },
                "_tftInstallSendTo": {
                    "type": "sendTo",
                    "label": "tftInstallSendTo",
                    "jsonData": "{\"tasmotaIP\": \"${data._selectIpForTools}\", \"internalServerIp\": \"${data.internalServerIp}\", \"useBetaTFT\": ${data.useBetaTFT} }",
                    "showProcess": true,
                    "command": "tftInstallSendTo",
                    "disabled": "!data._selectIpForTools || data._selectIpForTools.search(/^[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}$/g) === -1",
                    "variant": "contained",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                },
                "_header": {
                    "type": "header",
                    "size": 2,
                    "text": "Expert Mode",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                },
                "_resetTasmota": {
                    "type": "sendTo",
                    "label": "resetTasmota",
                    "jsonData": "{\"ip\": \"${data._selectIpForTools}\"}",
                    "openUrl": true,
                    "command": "openLinkToTasmota",
                    "disabled": "!data._selectIpForTools",
                    "variant": "contained",
                    "expertMode": true,
                    "confirm": {
                        "title": "NSPanel Reset!!!",
                        "text": "Reset NSPanel configuration on the external device to factory defaults(Wifi not touched).",
                        "ok": "Reset",
                        "cancel": "Cancel"
                    },
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                },
                "useBetaTFT": {
                    "type": "checkbox",
                    "label": "useBetaTFT",
                    "default": false,
                    "newLine": true,
                    "expertMode": true,
                    "style": {
                        "color": "red"
                    },
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                }
            }
        },
