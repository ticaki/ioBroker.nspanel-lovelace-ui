                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                },
                "_tasmotaRestartSendTo": {
                    "type": "sendTo",
                    "label": "Restart",
                    "jsonData": "{ \"tasmotaIP\": \"${data._selectIpForTools}\"}",
                    "showProcess": true,
                    "command": "tasmotaRestartSendTo",
                    "disabled": "!data._selectIpForTools || data._selectIpForTools.search(/^[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}$/g) === -1",
                    "variant": "outlined",
                    "confirm": {
                        "title": "Tasmota restart",
