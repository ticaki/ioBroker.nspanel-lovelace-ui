        "overview": {
            "type": "panel",
            "label": "Panels",
            "hidden": "(!data.mqttServer && !data.mqttIp ) || !data.mqttPort || !data.mqttUsername || !data.mqttPassword",
            "items": {
                "_useBetaTFTText": {
                    "type": "staticText",
                    "label": "useBetaTFTText",
                    "text": "useBetaTFTText",
                    "style": {
                        "color": "red"
                    },
                    "newLine": true,
                    "hidden": "!data.useBetaTFT",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                },
                "_header3": {
                    "type": "header",
                    "text": "nsPanelInitLabel",
                    "size": 4,
                    "newLine": true
                },
                "internalServerIp": {
                    "type": "ip",
                    "label": "internalServerIp",
                    "noInternal": true,
                    "listenOnAllPorts": false,
                    "onlyIp4": true,
                    "validator": "data.internalServerIp && (data.internalServerIp.search(/^[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}$/g) !== -1 && data.internalServerIp !== '127.0.0.1' )",
                    "hidden": "!data.mqttServer",
                    "default": "",
                    "xs": 12,
                    "sm": 6,
                    "md": 4,
                    "lg": 3,
                    "xl": 3
                },
                "_tasmotaIP": {
                    "type": "text",
                    "label": "ipFromPanel",
                    "validator": "typeof data._tasmotaIP === 'string' && data._tasmotaIP.search(/^[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}$/g) !== -1",
                    "validatorErrorText": "mustBeIp",
                    "default": "",
                    "xs": 12,
                    "sm": 6,
                    "md": 4,
                    "lg": 3,
                    "xl": 3
                },
                "_tasmotaName": {
                    "type": "text",
                    "label": "panelName",
                    "validator": "",
                    "default": "",
                    "xs": 12,
                    "sm": 6,
                    "md": 4,
                    "lg": 3,
                    "xl": 3
                },
                "_tasmotaTopic": {
                    "type": "text",
                    "label": "panelTopic",
                    "validator": "data._tasmotaTopic && data._tasmotaTopic.length > 3 && data._tasmotaTopic.search(/^[a-zA-Z][\\w/]+$/g) !== -1",
                    "default": "",
                    "xs": 12,
                    "sm": 6,
                    "md": 4,
                    "lg": 3,
                    "xl": 3
                },
                "timezone": {
                    "type": "selectSendTo",
                    "command": "getTimeZones",
                    "jsonData": "{\"ip\": \"${data.internalServerIp}\"}",
                    "label": "timezone",
                    "freeSolo": true,
                    "newLine": true,
                    "xs": 12,
                    "sm": 6,
                    "md": 4,
                    "lg": 3,
                    "xl": 3
                },
                "_nsPanelInit": {
                    "type": "sendTo",
                    "label": "nsPanelInit",
                    "jsonData": "{ \"tasmotaName\": \"${data._tasmotaName}\", \"tasmotaIP\": \"${data._tasmotaIP}\", \"tasmotaTopic\":\"${data._tasmotaTopic}\", \"mqttServer\": \"${data.mqttServer}\", \"mqttIp\": \"${data.mqttIp}\", \"mqttPort\": ${data.mqttPort}, \"mqttUsername\": \"${data.mqttUsername}\", \"mqttPassword\": \"${data.mqttPassword}\", \"internalServerIp\": \"${data.internalServerIp}\", \"useBetaTFT\": ${data.useBetaTFT} }",
                    "showProcess": true,
                    "command": "nsPanelInit",
                    "disabled": "return !data._tasmotaIP || (data.mqttServer && (!data.internalServerIp || data.internalServerIp.search(/^[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}$/g) === -1 || data.internalServerIp === '127.0.0.1')) ||!data._tasmotaIP || data._tasmotaIP.search(/^[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}$/g) === -1 || !data._tasmotaTopic || data._tasmotaTopic.search(/^[a-zA-Z][\\w/]+$/g) === -1 || (!data.mqttServer && !data.mqttIp ) || !data.mqttPort || !data.mqttUsername || !data.mqttPassword || !data._tasmotaName",
                    "newLine": true,
                    "variant": "contained",
                    "useNative": true,
                    "confirm": {
                        "title": "NSPanel Configuration",
                        "text": "Added the Tasmota NSPanel to Adapter. Overwrite settings with default values. Takes about 30 seconds.",
                        "ok": "Start",
                        "cancel": "Cancel"
                    }
                },
