{
    "i18n": true,
    "type": "tabs",
    "items": {
        "general": {
            "type": "panel",
            "label": "general",
            "items": {
                "mqttServer": {
                    "type": "checkbox",
                    "label": "mqttServer",
                    "default": true,
                    "newLine": true,
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                },
                "mqttIp": {
                    "type": "text",
                    "label": "mqttIp",
                    "newLine": true,
                    "disabled": "data.mqttServer",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                },
                "mqttPort": {
                    "type": "text",
                    "label": "mqttPort",
                    "newLine": false,
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                },
                "mqttUsername": {
                    "type": "text",
                    "label": "mqttUser",
                    "newLine": true,
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                },
                "mqttPassword": {
                    "type": "password",
                    "label": "mqttPassword",
                    "newLine": false,
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                },
                "_header": {
                    "type": "header",
                    "label": "Not used",
                    "size": 3,
                    "text": "Not used",
                    "hidden": "true",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                },
                "topic": {
                    "type": "text",
                    "label": "topic",
                    "newLine": true,
                    "hidden": "true",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                },
                "name": {
                    "type": "text",
                    "label": "name",
                    "newLine": false,
                    "hidden": "true",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                },
                "timeout": {
                    "type": "number",
                    "label": "timeout",
                    "newLine": true,
                    "hidden": "true",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                },
                "mediaid": {
                    "type": "text",
                    "label": "mediaid",
                    "newLine": false,
                    "hidden": "true",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                },
                "onlyStartFromSystemConfig": {
                    "type": "checkbox",
                    "label": "onlyStartFromSystemConfig",
                    "default": true,
                    "newLine": true,
                    "hidden": "true",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                },
                "Testconfig2": {
                    "type": "jsonEditor",
                    "label": "Testconfig2",
                    "hidden": "true",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                },
                "logUnknownTokens": {
                    "type": "checkbox",
                    "label": "logUnknownTokens",
                    "default": false,
                    "newLine": true,
                    "hidden": "true",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                },
                "doubleClickTime": {
                    "type": "number",
                    "label": "doubleClickTime",
                    "default": 350,
                    "min": 1,
                    "max": 2000,
                    "newLine": true,
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                }
            }
        },
        "overview": {
            "type": "panel",
            "label": "overview",
            "items": {
                "panels": {
                    "type": "table",
                    "label": "panels",
                    "sm": 12,
                    "md": 12,
                    "lg": 12,
                    "xl": 12,
                    "uniqueColumns": [
                        "name",
                        "topic"
                    ],
                    "items": [
                        {
                            "type": "text",
                            "attr": "id",
                            "label": "Id",
                            "validator": "data.id",
                            "validatorNoSaveOnError": true,
                            "readOnly": true
                        },
                        {
                            "type": "text",
                            "attr": "name",
                            "label": "panelName",
                            "readOnly": false,
                            "validatorErrorText": "mustBeUnique"
                        },
                        {
                            "type": "text",
                            "attr": "topic",
                            "label": "panelTopic",
                            "readOnly": false,
                            "validatorErrorText": "mustBeUnique"
                        },
                        {
                            "attr": "_RefreshDevices",
                            "type": "sendTo",
                            "label": "Refresh Device",
                            "command": "RefreshDevices",
                            "jsonData": "{\"id\": \"${data.id}\", \"name\": \"${data.name}\", \"topic\": \"${data.topic}\" }",
                            "showProcess": true,
                            "useNative": true,
                            "timeout": 8000,
                            "disabled": "!data.topic || !data.name"
                        }
                    ]
                },
                "pw1": {
                    "type": "password",
                    "label": "Service-Pin (numbers only)",
                    "newLine": true,
                    "validator": "data.pw1.search(/[^0-9]+/g) === -1",
                    "validatorNoSaveOnError": true,
                    "validatorErrorText": "mustBeNumber",
                    "xs": 12,
                    "sm": 8,
                    "md": 6,
                    "lg": 4,
                    "xl": 3
                }
            }
        },
        "pageQR": {
            "type": "panel",
            "label": "+",
            "items": {
                "pageQRdata": {
                    "type": "table",
                    "label": "pageQRdata",
                    "sm": 12,
                    "md": 12,
                    "lg": 12,
                    "xl": 12,
                    "uniqueColumns": [
                        "pageName"
                    ],
                    "items": [
                        {
                            "attr": "pageName",
                            "type": "text",
                            "label": "pageName",
                            "width": "10%",
                            "default": "Unique Name"
                        },
                        {
                            "attr": "headline",
                            "type": "text",
                            "label": "Headline",
                            "width": "10%",
                            "default": ""
                        },
                        {
                            "attr": "SSIDURLTEL",
                            "type": "text",
                            "label": "SSIDURLTEL",
                            "width": "40%",
                            "default": ""
                        },
                        {
                            "attr": "selType",
                            "type": "select",
                            "label": "pageQRselType",
                            "default": 0,
                            "options": [
                                {
                                    "label": "FREE",
                                    "value": 0
                                },
                                {
                                    "label": "Wifi",
                                    "value": 1
                                },
                                {
                                    "label": "URL",
                                    "value": 2
                                }
                                ,
                                {
                                    "label": "TEL",
                                    "value": 3
                                }       
                            ]
                        },
                        {
                            "type": "select",
                            "label": "wlantype",
                            "disabled": "data.selType !== 1",
                            "attr": "wlantype",
                            "options": [
                                {"label": "nopass", "value": "nopass"},
                                {"label": "WPA", "value": "WPA"},
                                {"label": "WPA2", "value": "WPA2"},
                                {"label": "WPA3", "value": "WPA3"},
                                {"label": "WEP", "value": "WEP"}
                            ]
                        },
                        {
                            "attr": "qrPass",
                            "type": "select",
                            "label": "qrPass",
                            "disabled": "data.selType !== 1",
                            "options": [
                                {
                                    "label": "none",
                                    "value": 0
                                },
                                {
                                    "label": "Password1",
                                    "value": 1
                                },
                                {
                                    "label": "Password2",
                                    "value": 2
                                },
                                {
                                    "label": "Password3",
                                    "value": 2
                                }
                            ]
                        },
                        {
                            "attr": "wlanhidden",
                            "type": "checkbox",
                            "label": "wlanhidden",
                            "disabled": "data.selType !== 1",
                            "default": false
                        },
                        {
                            "attr": "setState",
                            "type": "objectId",
                            "label": "Switch",
                            "filterFunc": "return obj.common.type === 'boolean' && obj.common.role === 'switch' && obj.common.write"
                        }    
                    ]
                },
                "pageQRpwd1": {
                    "type": "password",
                    "label": "Password1",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                },
                "pageQRpwd2": {
                    "type": "password",
                    "label": "Password2",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                },
                "pageQRpwd3": {
                    "type": "password",
                    "label": "Password3",
                    "xs": 12,
                    "sm": 12,
                    "md": 6,
                    "lg": 4,
                    "xl": 4
                }
            }
        }
    }
}